    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "28"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2017.03"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
