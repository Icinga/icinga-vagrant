{
  "name": "golja-influxdb",
  "version": "5.0.0",
  "author": "Dejan Golja",
  "summary": "Manage InfluxDB version 1.0.0+. For previous supported version check README.md",
  "license": "Apache-2.0",
  "source": "https://github.com/codylane/golja-influxdb",
  "project_page": "https://github.com/codylane/golja-influxdb",
  "issues_url": "https://github.com/codylane/golja-influxdb/issues",
  "tags": [
    "influxdb",
    "metrics"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 <4.13.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=1.8.0 <3.0.0"
    }
  ]
}
